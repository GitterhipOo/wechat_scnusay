.t-link {
  position: relative;
  display: flex;
  align-items: center;
  box-sizing: content-box;
}
.t-link--underline::after {
  content: '';
  position: absolute;
  left: 0;
  right: 0;
  height: 0;
  bottom: 0;
  opacity: 1;
  border-bottom: 1px solid #cd0be7;
}
.t-link__prefix-icon:not(:empty) + .t-link__content:not(:empty) {
  padding-left: 8rpx;
}
.t-link__content:not(:empty) + .t-link__suffix-icon:not(:empty) {
  padding-left: 8rpx;
}
