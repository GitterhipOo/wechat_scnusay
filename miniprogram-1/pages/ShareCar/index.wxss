/* 帖子数据 */
.box {
    width: 96%;
    position: relative;
    margin: 35rpx auto;
    background-color: #fff;
    border-radius: 25rpx;
    box-shadow: -1px 2px 6px 0px rgba(0, 0, 0, 0.4);
    height:480rpx;
    border: rgba(116, 202, 202, 0.6) solid 1rpx;
}

.box_my {
    width: 96%;
    position: relative;
    margin: 35rpx auto;
    background-color: #fff;
    border-radius: 25rpx;
    box-shadow: -1px 2px 6px 0px rgba(0, 0, 0, 0.4);
    height:530rpx;
    border: rgba(116, 202, 202, 0.6) solid 1rpx;
}

.box1 {
    height: 100rpx;
    width: 500rpx;
    padding-top: 23rpx;
    padding-left: 20rpx;
}

.box1 image {
    float: left;
    margin: 0rpx 0 5rpx 14rpx;
    width: 90rpx;
    height: 90rpx;
    border-radius: 90rpx;
}


.box1 .text1 {
    float: left;
    margin-top: 7rpx;
    margin-left: 20rpx;
    font-weight: 700;
    font-size: 35rpx;
    width: 380rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    flex-flow:row wrap;
    word-break: break-all;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
}

.box1 .text2 {
    float: left;
    margin-left: 20rpx;
    margin-top: 5rpx;
    font-size: 23rpx;
    color: #aaa;
    font-weight: 520;
}

.box-way {
    position: absolute;
    right: 35rpx;
    top: 36rpx;
    color: rgb(91, 102, 106);
    font-size: 36rpx;
    font-weight: 700;
}

.box2 {
    margin: 5rpx auto 0rpx auto;
    width: 94%;
    padding-top: 5rpx;
    padding-bottom: 7.5rpx;
    background-color: #ffffff;
    border-radius: 20rpx;
    border: 4rpx solid #c5dddd;
}

